{
  "name": "moodle-reports-frontend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "make": "elm-make ./src/Main.elm --output ./dist/app.js",
    "optimise": "cat ./dist/app.js | uglifyjs --compress --output ./dist/app.min.js",
    "compress": " gzip --best --force ./dist/app.min.js --to-stdout > ./dist/app.min.js.gz",
    "build": "npm run make && npm run optimise && npm run compress",
    "live": "elm-live ./src/Main.elm --output=./dist/app.js --dir=./dist --open"
  },
  "devDependencies": {
    "elm": "*",
    "elm-format": "*",
    "elm-live": "*",
    "uglify-js": "*"
  },
  "author": "Robert Porter",
  "private": true
}
